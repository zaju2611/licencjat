<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>geoDraw</title>
    <!-- @include 'links.kit' -->
</head>

<body>
    <!-- @include 'nav.kit' -->

    <main class="mainFormulas">
        <section class="mainFormulas_section">
            <div class="wrapper">
                <h2 class="title">Figury płaskie</h2>
                <div class="plane-figures">
                    <div class="plane-figures_card">
                        <h3 class="card-title">Kwadrat</h3>
                        <img src="./dist/img/kwadrat.jpg" alt="kwadrat">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = a<sup>2</sup></p>
                            <p class="field"><span class="text">P</span> = <sup>1</sup>/<sub>2</sub>*d<sup>2</sup></p>
                            <p class="field"><span class="text">Obw</span> = 4*a</p>
                            <p class="field"><span class="text">d</span> = &#8730;<span>a<sup>2</sup></span></p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Prostokąt</h3>
                        <img src="./dist/img/prostokąt.jpg" alt="prostokąt">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = a*b</p>
                            <p class="field"><span class="text">Obw</span> = 2*a +2*b</p>
                            <p class="field"><span class="text">d</span> =
                                &#8730;<span>(a<sup>2</sup>+b<sup>2</sup>)</span>
                            </p>
                        </div>
                    </div>

                    <div class="plane-figures_card">
                        <h3 class="card-title">Trójkąt</h3>
                        <img src="./dist/img/trójkąt.jpg" alt="trojkąt">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = <sup>1</sup>/<sub>2</sub>*a*h</p>
                            <p class="field"><span class="text">P</span> = &#8730;<span>(p*(p-a)*(p-b)*(p-c))</span></p>
                            <p class="heron">gdzie <span class="text">p</span> = <sup>1</sup>/<sub>2</sub>*(a+b+c) </p>
                            <p class="field"><span class="text">P</span> =
                                <sup>1</sup>/<sub>2</sub>*a*b*sin(<span class="symbol">&#947;</span>) =
                                <sup>1</sup>/<sub>2</sub>*b*c*sin(&#945;) = <sup>1</sup>/<sub>2</sub>*a*c*sin(&#946;)
                            </p>
                            <p class="field"><span class="text">Obw</span> = a+b+c</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Równoległobok</h3>
                        <img src="./dist/img/rownoleglobok.jpg" alt="rownoleglobok">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = a*h = a*b*sin(&#945;)</p>
                            <p class="field"><span class="text">P</span> =
                                <sup>1</sup>/<sub>2</sub>*d<sub>1</sub>*d<sub>2</sub>*sin(&#947;)
                            </p>
                            <p class="field"><span class="text">Obw</span> = 2*a+2*b</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Romb</h3>
                        <img src="./dist/img/romb.jpg" alt="romb">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = a*h = a<sup>2</sup>*sin(&#945;)</p>
                            <p class="field"><span class="text">P</span> =
                                <sup>1</sup>/<sub>2</sub>*d<sub>1</sub>*d<sub>2</sub>
                            </p>
                            <p class="field"><span class="text">Obw</span> = 4*a</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Trapez</h3>
                        <img src="./dist/img/trapez.jpg" alt="trapez">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = <sup>1</sup>/<sub>2</sub>*(a+b)*h</p>
                            <p class="field"><span class="text">Obw</span> = a+b+c+d</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Koło</h3>
                        <img src="./dist/img/koło.jpg" alt="kolo">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = &#960;*r<sup>2</sup></p>
                            <p class="field"><span class="text">Obw</span> = 2*&#960;*r</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Deltoid</h3>
                        <img src="./dist/img/deltoid.jpg" alt="deltoid">
                        <div class="plane-figures_card-formulas">
                            <p class="field"> <span class="text">P</span> =
                                <sup>1</sup>/<sub>2</sub>*d<sub>1</sub>*d<sub>2</sub>
                            </p>
                            <p class="field"><span class="text">P</span> = a*b*sin(&#945;)</p>
                            <p class="obw"><span class="text">Obw</span> = 2*a+2*b</p>
                        </div>
                    </div>
                </div>
                <h2 class="title">Figury przestrzenne</h2>
                <div class="plane-figures">
                    <div class="plane-figures_card">
                        <h3 class="card-title">Sześcian</h3>
                        <img src="./dist/img/szescian.jpg" alt="kwadrat">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P<sub>c</sub></span> = 6*a<sup>2</sup></p>
                            <p class="field"><span class="text">V</span> = a<sup>3</sup></p>
                            <p class="field"><span class="text">d</span> = a*&#8730;3</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Prostopadłościan</h3>
                        <img src="./dist/img/prostopadloscian.jpg" alt="prostopadłościan">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P<sub>c</sub></span> = 2*a*b+2*a*c+2*b*c =
                                2*(a*b+a*c+b*c)</p>
                            <p class="field"><span class="text">V</span> = a*b*c</p>
                            <p class="field"><span class="text">d</span> =
                                &#8730;(a<sup>2</sup>+b<sup>2</sup>+c<sup>2</sup>)
                            </p>
                        </div>
                    </div>

                    <div class="plane-figures_card">
                        <h3 class="card-title">Graniastosłup prawidłowy sześciokątny</h3>
                        <img src="./dist/img/graniastoslup_prawidlowy_szesciokatny.jpg" alt="trojkąt">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P<sub>p</sub></span> =
                                6*<sup>(a<sup>2</sup>*&#8730;3)</sup>/<sub>4</sub> =
                                3*<sup>(a<sup>2</sup>*&#8730;3)</sup>/<sub>2</sub></p>
                            <p class="field"><span class="text">P<sub>b</sub></span> =
                                6*a*h</p>
                            <p class="field"><span class="text">P<sub>c</sub></span> =
                                2*<sup>3*(a<sup>2</sup>*&#8730;3)</sup>/<sub>2</sub>+6*a*h
                            </p>
                            <p class="field"><span class="text">V</span> =
                                <sup>(3*a<sup>2</sup>*&#8730;3)</sup>/<sub>2</sub>*h
                            </p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Walec</h3>
                        <img src="./dist/img/walec.jpg" alt="walec">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P<sub>p</sub></span> = &#960;*r<sup>2</sup></p>
                            <p class="field"><span class="text">P<sub>b</sub></span> =
                                2*&#960;*r*h
                            </p>
                            <p class="field"><span class="text">P<sub>c</sub></span> = 2*P<sub>p</sub>+P<sub>b</sub> =
                                2*&#960;*r<sup>2</sup>+2*&#960;*r*h = 2*&#960;*r*(r+h)
                            </p>
                            <p class="field"><span class="text">V</span> = p<sub>p</sub>*h = &#960;*r<sup>2</sup>*h</p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Kula</h3>
                        <img src="./dist/img/kula.jpg" alt="kula">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P</span> = 4*&#960;*r<sup>2</sup></p>
                            <p class="field"><span class="text">V</span> =
                                <sup>4</sup>/<sub>3</sub>*&#960;*r<sup>3</sup>
                            </p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Stożek</h3>
                        <img src="./dist/img/stożek.jpg" alt="stożek">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P<sub>p</sub></span> = &#960;*r<sup>2</sup>
                            </p>
                            <p class="field"><span class="text">P<sub>b</sub></span> = &#960;*r*l
                            </p>
                            <p class="field"><span class="text">P<sub>c</sub></span> =
                                P<sub>p</sub>+P<sub>b</sub> = &#960;*r<sup>2</sup>+&#960;*r*l
                            </p>
                            <p class="field"><span class="text">V</span> = <sup>1</sup>/<sub>3</sub>*P<sub>p</sub>*h =
                                <sup>(&#960;*r<sup>2</sup>*h)</sup>/<sub>3</sub>
                            </p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Ostrosłup prawidłowy czworokątny</h3>
                        <img src="./dist/img/ostroslup_prawidlowy_czworokatny.jpg"
                            alt="ostrosłup prawidłowy czworokątny">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text">P<sub>p</sub></span> =
                                a<sup>2</sup></p>
                            <p class="field"><span class="text">P<sub>b</sub></span> =
                                4*<sup>1</sup>/<sub>2</sub>*a*h</p>
                            <p class="field"><span class="text">P<sub>c</sub></span> = P<sub>p</sub>+P<sub>b</sub> =
                                a<sup>2</sup>+4*<sup>1</sup>/<sub>2</sub>*a*h = a<sup>2</sup>+2*a*h</p>
                            <p class="field"><span class="text">V</span> = <sup>1</sup>/<sub>3</sub>*P<sub>p</sub>*H =
                                <sup>1</sup>/<sub>3</sub>*a<sup>2</sup>*H
                            </p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Ostrosłup prawidłowy trójkątny</h3>
                        <img src="./dist/img/ostroslup_prawidlowy_trojkatny.jpg" alt="ostrosłup prawidłowy trojkątny">
                        <div class="plane-figures_card-formulas">
                            <p class="field"><span class="text"> P<sub>p</sub></span> =
                                <sup>(a<sup>2</sup>&#8730;3)</sup>/<sub>4</sub>
                            </p>
                            <p class="field"><span class="text">P<sub>b</sub></span> =
                                3*<sup>1</sup>/<sub>2</sub>*a*h
                            </p>
                            <p class="field"><span class="text">P<sub>c</sub></span> = P<sub>p</sub>+P<sub>b</sub> =
                                <sup>(a<sup>2</sup>&#8730;3)</sup>/<sub>4</sub>+3*<sup>1</sup>/<sub>2</sub>*a*h
                            </p>
                            <p class="field"><span class="text">V</span> = <sup>1</sup>/<sub>3</sub>*P<sub>p</sub>*H =
                                <sup>1</sup>/<sub>3</sub>*<sup>(a<sup>2</sup>*&#8730;3)</sup>/<sub>4</sub>*H =
                                <sup>(a<sup>2</sup>*H*&#8730;3)</sup>/<sub>12</sub>
                            </p>
                        </div>
                    </div>
                    <div class="plane-figures_card">
                        <h3 class="card-title">Czworościan foremny</h3>
                        <img src="./dist/img/czworoscian_foremny.jpg" alt="czworościan foremny">
                        <div class="plane-figures_card-formulas">
                            <p class="field"> <span class="text">P<sub>c</sub></span> =
                                a<sup>2</sup>*&#8730;3
                            </p>
                            <p class="field"><span class="text">V</span> =
                                <sup>(a<sup>3</sup>*&#8730;2)</sup>/<sub>12</sub>
                            </p>
                            <p class="field"><span class="text">H</span> = <sup>(a*&#8730;6)</sup>/<sub>3</sub></p>
                            <p class="field"><span class="text">h</span> = <sup>(a*&#8730;3)</sup>/<sub>2</sub></p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>

    <!-- @include 'footer.kit' -->

    <script type="module" src="../src/js/logout.js"></script>
    <script src="../src/js/checkUser.js"></script>
    <script src="../src/js/main.js"></script>
</body>

</html>